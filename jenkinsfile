// This shows a simple example of how to archive the build output artifacts.
node {
        stage('Checkout SCM'){
                git branch: 'SP2', url: 'https://github.com/rabia-gh/angular-ssr-starter.git'
        }
        stage('Build'){
                bat"npm run build:ssr"
        }
        stage('Deploy'){
                bat "pm2 restart all"
        }
}
